usia = 2022 - int(umur)
print(usia)
print(nama_awal + ' [' + nama_akhir + '] suka warna ' + favorite_color + 'yang berusia ' + umur)
weight_kg = input('Berat (kg):')
weight_lbs = int(weight_kg) * 2
print(weight_lbs)
terakhir = input ('berarti kamu ' + nama_awal + ' [' + nama_akhir + ']' + 'yang berusia' + umur)
